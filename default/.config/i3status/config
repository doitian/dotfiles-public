general {
        colors = true
        interval = 5
}

order += "memory"
order += "load"
order += "tztime local"
order += "wireless wlo1"

wireless wlo1 {
        format_up = "W: %ip"
        format_down = "W: down"
}

tztime local {
        format = "%b %-d, %a %H:%M"
}

load {
        format = "%1min, %5min, %15min"
}

memory {
        format = "%used"
        threshold_degraded = "10%"
        format_degraded = "%free"
}
