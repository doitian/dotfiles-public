#!/bin/bash

if [[ $OSTYPE == darwin* ]]; then
  pbpaste
elif [[ $OSTYPE == cygwin* ]]; then
  cat /dev/clipboard
elif command -v xclip &> /dev/null; then
  xclip -out -selection clipboard
elif command -v xsel &> /dev/null; then
  xsel --clipboard --output
else
  echo "Platform $OSTYPE not supported!" >&2
  exit 1
fi
