{
  "name": "snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "global",
          "all"
        ],
        "path": "./snippets/global.code-snippets"
      },
      {
        "language": "markdown",
        "path": "./snippets/markdown.json"
      },
      {
        "language": [
          "shellscript",
          "shell",
          "sh",
          "zsh"
        ],
        "path": "./snippets/sh.json"
      },
      {
        "language": "tmux",
        "path": "./snippets/tmux.json"
      },
      {
        "language": [
          "java"
        ],
        "path": "./snippets/java.json"
      },
      {
        "language": [
          "sshconfig"
        ],
        "path": "./snippets/sshconfig.json"
      },
      {
        "language": [
          "python"
        ],
        "path": "./snippets/python.json"
      }
    ]
  }
}
