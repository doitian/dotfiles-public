{
  "Markdown ➤ Journal Section": {
    "prefix": "jsec",
    "body": ["### $CURRENT_HOUR:$CURRENT_MINUTE$1", "", "$0"]
  },

  "Markdown ➤ Insert code block": {
    "prefix": "codeblock",
    "body": ["```${1:language}", "$0", "```"],
    "description": "Insert fenced code block"
  },

  "Markdown ➤ Pandoc Metadata": {
    "prefix": "pandocmeta",
    "body": [
      "---",
      "title: ${1:$TM_FILENAME_BASE}",
      "author: $2",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "---",
      "$0"
    ],
    "description": "Insert fenced code block"
  },

  "Obsidian ➤ Docklet": {
    "prefix": "docklet",
    "body": [
      "# $TM_FILENAME_BASE",
      "",
      "## Metadata",
      "",
      "**Status**:: #i",
      "**Zettel**:: #zettel/fleeting",
      "**Created**:: [[$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE]]",
      "",
      "## Synopsis"
    ]
  }
}
